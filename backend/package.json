{
  "name": "eigenskills-backend",
  "version": "0.1.0",
  "description": "Orchestration backend for EigenSkills platform",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@layr-labs/ecloud-sdk": "^0.3.3",
    "better-sqlite3": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.3.1",
    "ethers": "^6.13.0",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.0.0",
    "node-telegram-bot-api": "^0.67.0",
    "siwe": "^2.3.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/better-sqlite3": "^7.6.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  }
}
